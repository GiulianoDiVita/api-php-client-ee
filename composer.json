{
  "name": "akeneo/api-php-client-ee",
  "description": "Akeneo PIM ENTERPRISE client for the API",
  "license": "OSL-3.0",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Akeneo",
      "homepage": "http://www.akeneo.com"
    }
  ],
  "autoload": {
    "psr-4": {
